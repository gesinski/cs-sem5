BEGIN ! This is just a test program;
   Class Glyph;
      Virtual: Procedure print Is Procedure print;;
   Begin
   End;
   
   Glyph Class Char (c); comment this is an example class; 
      Character c;
   Begin
      Procedure print;
        OutChar(c);
   End;
   
   Glyph Class Line (elements);
      Ref (Glyph) Array elements;
   Begin
      Procedure print;
      Begin
         Integer i;
         For i := 1 Step 1 Until UpperBound (elements, 1) Do
            elements (i).print;
         OutImage;
      END print;
   End;
   
   Ref (Glyph) rg;
   Ref (Glyph) Array rgs (1 : 4);
   
   ! Main program;
   OutText("Testing this ""new"" program");
   REAL r;
   r := 3.1415;
   rgs (1) :- New Char ('A');
   rgs (2) :- New Char ('b');
   rgs (3) :- New Char ('b');
   rgs (4) :- New Char ('a');
   rg :- New Line (rgs);
   rg.print;
End;
